package CAMs;

public interface EnquirySuggestionHandler {
	public void submitEnquiry(Camp camp, Enquiry enquiry, Student student);
	public void viewEnquiries(Student student);
	public void editEnquiry(Enquiry oldEnquiry, String newEnquiryText, Student student);
    public void deleteEnquiry(Enquiry oldEnquiry, Student student);
}
